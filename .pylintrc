[MASTER]
ignore-paths=^[.]venv/.*$,^mypy/.*$
recursive=y

[FORMAT]
good-names=r,g,b,n,i,j,x,y,z
ignore-long-lines=^\s*(#\s*)?<?https?://\S+>?$|^\s*(\w*\s*=\s*)?(return\s*)?(f)?(\"|\').*(\"|\'),?\s*$

[MESSAGES CONTROL]
disable=fixme
